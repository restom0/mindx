<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
            <th scope="col">Handle</th>
        </tr>
    </thead>
    <tbody>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Description</th>
                    <th scope="col">Image</th>
                </tr>
            </thead>
            <tbody>
                <% if(courses.length> 0) {
                    courses.forEach(function(data) {
                    %>
                    <tr>
                        <td>
                            <%= data.id %>
                        </td>
                        <td>
                            <%= data.name %>
                        </td>
                        <td>
                            <%= data.description %>
                        </td>
                        <td>
                            <%= data.image %>
                        </td>
                    </tr>
                    <% }); } else { %>
                        <tr>
                            <td colspan="4">No Data Found</td>
                        </tr>
                        <% } %>
            </tbody>
        </table>